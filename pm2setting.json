{
  "apps": [
    {
      "name": "myapp",
      "script": "dist/server.js",
      "watch": false,
      "env": {
        "NODE_ENV": "development"
      },
      "env_production": {
        "NODE_ENV": "production"
      },
      "max_restarts": 5,
      "min_uptime": 5000,
      "restart_delay": 5000,
      "log_date_format": "MM-DD-YYYY HH:mm Z",
      "error_file": "logs/myapp_err.log",
      "out_file": "logs/myapp_out.log"
    }
  ]
}
